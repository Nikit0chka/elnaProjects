TYPE
TYPE_AnalogLimitFlags : STRUCT (*Флаги интервалов, передаваемые с командой "сменить уставку*)
	NONE : BYTE := 0;
	LL : BYTE := 16#1; (*Нижняя граница пересчета*)
	LW : BYTE := 16#2; (*Нижняя пердупредительная граница *)
	HW : BYTE := 16#4; (*Верхняя предупредительная граница*)
	LA : BYTE := 16#8; (*Нижняя аварийная граница*)
	HA : BYTE := 16#10; (* Верхняя аварийная граница *)
	HL : BYTE := 16#20; (*Верхняя граница пересчета*)
	SipUP : BYTE := 16#40; (*Скорость изменения сигнала на возрастание*)
	SipDown : BYTE := 16#80; (*Скорость изменения сигнала на убывание*)
END_STRUCT;
END_TYPE
